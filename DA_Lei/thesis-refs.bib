@INPROCEEDINGS{DBLP:conf/cidr/MadhavanCDHJKY07,
  author = {Jayant Madhavan and Shirley Cohen and Xin Luna Dong and Alon Y. Halevy
	and Shawn R. Jeffery and David Ko and Cong Yu},
